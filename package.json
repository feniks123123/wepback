{
  "name": "pg",
  "version": "1.0.0",
  "description": "pg",
  "main": "index.js",
  "scripts": {
    "build:prodClient": "webpack --mode production --config webpack.config.js",
    "build:prodServer": "webpack --mode production --config webpack.config.server.js",
    "build:devClient": "webpack --watch --mode development --config webpack.config.js",
    "build:devServer": "webpack --watch --mode development --config webpack.config.server.js",
    "startServer:dev": "nodemon ./dist/server.js",
    "startServer:prod": "node ./dist/server.js",
    "start": "concurrently --kill-others \"npm run build:devClient\" \"npm run build:devServer\"  \"npm run startServer:dev\"",
    "build": "npm run build:prodClient && npm run build:prodServer"
  },
